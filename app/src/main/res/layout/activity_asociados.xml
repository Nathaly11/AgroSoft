<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroud_fondo"
    tools:context="com.example.nathaly.agrosoft3.Asociados">

    <RelativeLayout
        android:layout_width="350dp"
        android:layout_height="514dp"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="-2dp">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:lineSpacingExtra="12sp"
            android:text="Nuestra Comunidad"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            android:textColor="?android:attr/statusBarColor"
            android:textSize="24sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="match_parent"
            android:layout_height="160dp"
            android:paddingBottom="15dp"
            android:paddingTop="15dp"
            app:srcCompat="@drawable/asociados"
            android:layout_below="@+id/textView9"
            android:layout_alignParentStart="true" />

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
